{
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.3",
      "includeFiles": "sevenheaven-site/*"
    },
    "api/sevenheaven-site/build/*": {
      "runtime": "@vercel/static-build@2.0.3"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}